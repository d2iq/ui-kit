{
  "name": "@dcos/ui-kit",
  "version": "0.0.0-dev+semantically-released",
  "description": "DCOS UI Kit",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcos-labs/ui-kit"
  },
  "main": "dist/packages/index",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build:storybook": "build-storybook",
    "prebuild:storybook": "npm run build:design-tokens",
    "prebuild:design-tokens": "npm run clean:design-tokens",
    "build:design-tokens": "node ./packages/design-tokens/scripts/build.js",
    "clean": "rimraf dist",
    "clean:design-tokens": "rm -rf ./dist/design-tokens",
    "commitlint": "commitlint -e",
    "create:component": "./scripts/create-component",
    "deploy:storybook": "storybook-to-ghpages",
    "predeploy:storybook": "npm run prebuild:storybook",
    "prestart": "npm run build:design-tokens",
    "pretest": "npm run build:design-tokens",
    "predist": "npm run clean && npm run build:design-tokens",
    "dist": "tsc -p tsconfig.dist.json",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "setup:git-hooks": "./scripts/pre-install",
    "start": "NODE_ENV=development npm run storybook --root ./ ",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "typings": "dist/packages/index",
  "dependencies": {
    "emotion": "9.2.12",
    "memoize-one": "4.0.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-emotion": "9.2.12",
    "react-toggled": "1.2.7",
    "react-transition-group": "2.5.0",
    "react-virtualized": "9.20.1",
    "semantic-release": "^15.9.6"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.0",
    "@commitlint/config-conventional": "7.1.2",
    "@dcos/tslint-config": "0.1.1",
    "@storybook/addon-a11y": "4.0.4",
    "@storybook/addon-actions": "4.0.4",
    "@storybook/addon-info": "4.0.4",
    "@storybook/addon-knobs": "4.0.4",
    "@storybook/addon-links": "4.0.4",
    "@storybook/addon-options": "4.0.4",
    "@storybook/addons": "4.0.4",
    "@storybook/react": "4.0.4",
    "@storybook/storybook-deployer": "2.3.0",
    "@types/enzyme": "3.1.14",
    "@types/jest": "23.3.5",
    "@types/memoize-one": "3.1.1",
    "@types/node": "10.11.6",
    "@types/react": "16.4.16",
    "@types/react-dom": "16.0.9",
    "@types/react-test-renderer": "16.0.3",
    "@types/react-virtualized": "9.18.7",
    "@types/storybook__addon-actions": "3.4.1",
    "@types/storybook__addon-info": "3.4.2",
    "@types/storybook__addon-knobs": "3.4.1",
    "commander": "2.19.0",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "enzyme-to-json": "3.3.4",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "jest": "23.6.0",
    "jest-emotion": "9.2.11",
    "jest-runner-prettier": "0.2.6",
    "node": "10.11.0",
    "npm": "6.4.1",
    "prettier": "1.14.3",
    "prop-types": "15.6.2",
    "react-docgen-typescript-loader": "3.0.0",
    "react-test-renderer": "16.5.2",
    "rimraf": "2.6.2",
    "storybook-readme": "4.0.2",
    "style-dictionary": "^2.4.0",
    "ts-jest": "23.10.4",
    "ts-loader": "3.5.0",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-plugin-prettier": "2.0.0",
    "tslint-react": "3.6.0",
    "typescript": "3.1.2",
    "webpack": "3.11.0",
    "webpack-cli": "2.0.9",
    "webpack-dev-server": "2.11.2"
  }
}
