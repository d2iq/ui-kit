{
  "name": "@dcos/ui-kit",
  "version": "1.0.0",
  "description": "DCOS UI Kit",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcos-labs/ui-kit"
  },
  "main": "dist/packages/index",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build:storybook": "build-storybook",
    "clean": "rimraf dist",
    "commitlint": "commitlint -e",
    "create:component": "./scripts/create-component",
    "deploy:storybook": "storybook-to-ghpages",
    "predist": "npm run clean",
    "dist": "tsc -p tsconfig.dist.json",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "prerelease": "npm run dist",
    "release": "standard-version",
    "setup:git-hooks": "./scripts/pre-install",
    "start": "NODE_ENV=development npm run storybook --root ./ ",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "typings": "dist/components/index",
  "dependencies": {
    "emotion": "9.2.3",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-emotion": "9.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "7.0.0",
    "@commitlint/config-conventional": "7.0.1",
    "@dcos/tslint-config": "0.1.0",
    "@storybook/addon-a11y": "3.4.7",
    "@storybook/addon-actions": "3.4.7",
    "@storybook/addon-info": "3.4.7",
    "@storybook/addon-knobs": "3.4.7",
    "@storybook/addon-links": "3.4.7",
    "@storybook/addon-options": "3.4.7",
    "@storybook/addons": "3.4.7",
    "@storybook/react": "3.4.7",
    "@storybook/storybook-deployer": "2.3.0",
    "@types/enzyme": "3.1.10",
    "@types/jest": "23.0.2",
    "@types/node": "10.3.2",
    "@types/react": "16.3.17",
    "@types/react-dom": "16.0.6",
    "@types/react-test-renderer": "16.0.1",
    "@types/storybook__addon-actions": "3.0.3",
    "@types/storybook__addon-info": "3.2.3",
    "@types/storybook__addon-knobs": "3.3.1",
    "commander": "2.15.1",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "file-loader": "1.1.11",
    "fs-extra": "6.0.1",
    "jest": "23.1.0",
    "jest-runner-prettier": "0.2.4",
    "node": "10.4.0",
    "npm": "6.1.0",
    "prettier": "1.13.5",
    "prop-types": "15.6.1",
    "react-docgen-typescript-loader": "2.1.0",
    "react-test-renderer": "16.4.0",
    "rimraf": "2.6.2",
    "standard-version": "4.4.0",
    "storybook-readme": "3.3.0",
    "ts-jest": "22.4.6",
    "ts-loader": "4.4.1",
    "tslib": "1.9.2",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-eslint-rules": "5.3.1",
    "tslint-plugin-prettier": "1.3.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.1",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.4",
    "webpack-dev-server": "3.1.4"
  }
}
