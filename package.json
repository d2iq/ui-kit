{
  "name": "@dcos/ui-kit",
  "version": "0.0.0-dev+semantically-released",
  "description": "DCOS UI Kit",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcos-labs/ui-kit"
  },
  "main": "dist/packages/index",
  "module": "packages/index.ts",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build:storybook": "build-storybook",
    "clean": "rimraf dist",
    "commitlint": "commitlint -e",
    "create:component": "./scripts/create-component",
    "deploy:storybook": "storybook-to-ghpages",
    "predist": "npm run clean",
    "dist": "tsc -p tsconfig.dist.json",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "setup:git-hooks": "./scripts/pre-install",
    "start": "NODE_ENV=development npm run storybook --root ./ ",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist",
    "packages"
  ],
  "typings": "dist/packages/index",
  "dependencies": {
    "emotion": "9.2.8",
    "memoize-one": "4.0.2",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-emotion": "9.2.8",
    "react-virtualized": "9.20.1",
    "semantic-release": "^15.9.6"
  },
  "devDependencies": {
    "@commitlint/cli": "7.1.2",
    "@commitlint/config-conventional": "7.1.2",
    "@dcos/tslint-config": "0.1.1",
    "@storybook/addon-a11y": "3.4.10",
    "@storybook/addon-actions": "3.4.10",
    "@storybook/addon-info": "3.4.10",
    "@storybook/addon-knobs": "3.4.10",
    "@storybook/addon-links": "3.4.10",
    "@storybook/addon-options": "3.4.10",
    "@storybook/addons": "3.4.10",
    "@storybook/react": "3.4.10",
    "@storybook/storybook-deployer": "2.3.0",
    "@types/enzyme": "3.1.13",
    "@types/jest": "23.3.1",
    "@types/memoize-one": "3.1.1",
    "@types/node": "10.9.4",
    "@types/react": "16.4.13",
    "@types/react-dom": "16.0.7",
    "@types/react-test-renderer": "16.0.2",
    "@types/react-virtualized": "9.18.7",
    "@types/storybook__addon-actions": "3.4.1",
    "@types/storybook__addon-info": "3.4.1",
    "@types/storybook__addon-knobs": "3.4.1",
    "commander": "2.17.1",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "enzyme-to-json": "3.3.4",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "jest": "23.5.0",
    "jest-emotion": "9.2.7",
    "jest-runner-prettier": "0.2.6",
    "node": "10.10.0",
    "npm": "6.4.1",
    "prettier": "1.14.2",
    "prop-types": "15.6.2",
    "react-docgen-typescript-loader": "2.2.0",
    "react-test-renderer": "16.5.0",
    "rimraf": "2.6.2",
    "storybook-readme": "3.3.0",
    "ts-jest": "23.1.4",
    "ts-loader": "3.5.0",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-plugin-prettier": "1.3.0",
    "tslint-react": "3.6.0",
    "typescript": "3.0.3",
